<?xml version="1.0" encoding="utf-8" ?>

<help>
	<function>vertcat</function>
	
	<!-- one-line description of the function -->
	<info>
		Vertical concatenation for HashTable class.
	</info>
	
	<syntax>vertcat(h1,h2)</syntax>
    <syntax>[h1;h2]</syntax>
    <syntax>[h1;h2;h3]</syntax>

	<!-- long description of the function -->
	<descr>
        HashTable objects are concatenated to vertical cell arrays. Every matrix concatenation will be turned into an array.
        To call individual methods of HashTable from a cell use <tt>cellfun</tt> function.
    </descr>
	<!-- now describe input arguments -->
	
	<!-- mark inputs as mandatory by providing the "required" attribute -->
	<input required="true">
		<name>h1</name>
        <descr>HashTable object.</descr>
		<!-- any class name can be used, it can even be omitted -->
		<class name="HashTable"/>
	</input>
	<input required="false">
		<name>h2</name>
        <descr>HashTable object.</descr>
		<!-- any class name can be used, it can even be omitted -->
		<class name="HashTable"/>
	</input>

    
	<!-- multiple related function can be defined -->
    <related>HashTable</related>	
    <related>horzcat</related>	
    <related>elem</related>	
	
	<!-- multiple authors can be defined -->
	<author macro="author_herceg"/>

	<!-- finally, the text of the license -->
	<license macro="GPL2"/>
</help>
