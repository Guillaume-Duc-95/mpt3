<?xml version="1.0" encoding="utf-8" ?>

<help>
	<function>getValueByKey</function>
	
	<!-- one-line description of the function -->
	<info>
		Retrieve stored data for the element determined by given key.
	</info>
	
	<syntax>v = getValueByKey(h,key)</syntax>
	<syntax>v = h.getValueByKey(key)</syntax>

	<!-- long description of the function -->
	<descr>
        Retrieve value <tt>v</tt> for the element stored under the given <tt>key</tt>. If there is no elements
        stored under such a key, returns empty matrix.
	</descr>
	<!-- now describe input arguments -->
	
	<!-- mark inputs as mandatory by providing the "required" attribute -->
	<input required="true">
		<name>h</name>
        <descr>Initialized HashTable object.</descr>
		<!-- any class name can be used, it can even be omitted -->
		<class name="HashTable"/>
	</input>

    <input required="true">
		<name>key</name>
        <descr>Key associated to given element which can be a string or unique integer array.</descr>
		<!-- any class name can be used, it can even be omitted -->
		<class name="char or double"/>
	</input>

	<!-- now describe output arguments -->
	<output>
		<name>v</name>
        <descr>Data stored under the given key.</descr>
		<class name="struct"/>
	</output>

	<!-- we can provide multiple code examples -->
    <example>
            <!-- each example is a combination of descriptions and commands -->
        <descr>Construct empty table</descr>		
        <cmd>h = HashTable</cmd>
        <descr>Put one element inside the table with the <tt>key</tt> <em>hello</em>. </descr> 
        <cmd> h.put('hello',struct('data',[1,2,3])); </cmd>		
        <descr>Get the data for the key <em>hello</em> </descr>
        <cmd> h.getValueByKey('hello') </cmd>
	</example>

	<!-- multiple related function can be defined -->
	<related>HashTable</related>
    <related>put</related>
    <related>getIndex</related>
    <related>getKey</related>
    <related>getValueByIndex</related>
	
	
	<!-- multiple authors can be defined -->
	<author macro="author_herceg"/>

	<!-- finally, the text of the license -->
	<license macro="GPL2"/>
</help>
