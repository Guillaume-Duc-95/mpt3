<?xml version="1.0" encoding="utf-8" ?>

<help>
  <function>le</function>
  
  <info>
    Test if a polyhedron is contained inside polyhedron.
  </info>
  <syntax>S &lt;= P</syntax>
  <syntax>tf = P.contains(S)</syntax>
  <syntax>tf = contains(P, S)</syntax>
  
  <descr>
      Check if the polyhedron  <tt>S</tt> is contained inside the polyhedron <tt>P</tt>.
      The result it the logical statement if <i>S \subseteq P</i> and false otherwise.
  </descr>
  
  <input required="true">
    <name>P</name>
    <descr>Polyhedron in any format</descr>
    <class name="Polyhedron"/>
  </input>
  <input required="true">
    <name>S</name>
    <descr>Polyhedron in any format.</descr>
    <class name="Polyhedron"/>
  </input>
  
  <output>
    <name>tf</name>
    <descr>True if <i>S \subseteq P</i> and false otherwise.</descr>
    <class name="logical">
      <option>true</option>
      <option>false</option>
    </class>
  </output>
    
  <related>contains</related>
  <related>lt</related>
  <related>ge</related>
  <related>gt</related>
  
  <author macro="author_cnj"/>
  <license macro="GPL2"/>
</help>
